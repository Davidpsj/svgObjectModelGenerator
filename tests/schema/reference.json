{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "/reference",
	"title": "reference",
	"description": "A reference graphics object",
	"type": "object",
	"properties": {
		"reference": {
			"description": "Reference object specific properties",
			"type": "object",
			"properties": {
				"ref": {
					"type": "string",
					"description": "The unique id of the symbol in the global `symbols` object."
				},
				"x": {
					"description": "The horizontal position",
					"type": "number"
				},
				"meta": {
					"type": "object"
				},
				"y": {
					"description": "The vertical position",
					"type": "number"
				},
				"width": {
					"description": "The horizontal dimension",
					"type": "number"
				},
				"height": {
					"description": "The vertical dimension",
					"type": "number"
				}
			},
			"additionalProperties": false,
			"required": [ "ref" ]
		},
		"meta": {
			"type": "object"
		},
		"name": {
			"description": "The user specified name of the graphics object",
			"type": "string"
		},
		"style": { "$ref": "/style" },
		"transform": {
			"oneOf": [
				{ "$ref": "/transform2d" },
				{ "$ref": "/transform3d" }
			]
		},
		"type": {
			"description": "The type of the graphics object",
			"enum": [ "reference" ]
		},
		"visible": {
			"description": "Whether the graphics object is visible or not",
			"type": "boolean"
		},
		"visualBounds": { "$ref": "/bounds" }
	},
	"required": [ "reference", "type" ]
}
