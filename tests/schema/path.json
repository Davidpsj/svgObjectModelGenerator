{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "/path",
	"title": "path",
	"description": "A path shape graphics object",
	"type": "object",
	"properties": {
		"id": {
			"description": "A unique identifier with the document",
			"type": "string"
		},
		"meta": {
			"type": "object"
		},
		"name": {
			"description": "The user specified name of the graphics object",
			"type": "string"
		},
		"shape": {
			"type": "object",
			"description": "A representation of a path",
			"properties": {
				"meta": {
					"type": "object"
				},
				"path": {
					"description": "The SVG path data. (Example: `M235.521,29.696 C227.438,29.696 220.900,23.234 220.900,15.088z`)",
					"type": "string"
				},
				"preparedPath": {
					"description": "False if the path follows the unlimited syntax of SVG path data",
					"type": "boolean"
				},
				"winding": {
					"description": "The vertical coordinate of the first position",
					"enum": [ "evenodd", "nonzero" ]
				},
				"type": {
					"description": "The type of shape",
					"enum": [ "path" ]
				}
			},
			"additionalProperties": false,
			"required": [ "path", "type" ]
		},
		"style": { "$ref": "/style" },
		"transform": {
			"oneOf": [
				{ "$ref": "/transform2d" },
				{ "$ref": "/transform3d" }
			]
		},
		"type": {
			"description": "The type of the graphics object",
			"enum": [ "shape" ]
		},
		"visible": {
			"description": "Whether the graphics object is visible or not",
			"type": "boolean"
		},
		"visualBounds": { "$ref": "/bounds" }
	},
	"additionalProperties": false,
	"required": [ "shape", "type" ]
}
