{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "/bounds",
	"title": "bounds",
	"description": "The boundaries object",
	"type": "object",
	"properties": {
		"x": {
			"description": "The horizontal position",
			"type": "number"
		},
		"y": {
			"description": "The vertical position",
			"type": "number"
		},
		"meta": {
			"type": "object"
		},
		"width": {
			"description": "The horizontal dimension",
			"type": "number"
		},
		"height": {
			"description": "The vertical dimension",
			"type": "number"
		}
	},
	"additionalProperties": false,
	"required": [ "width", "height" ]
}