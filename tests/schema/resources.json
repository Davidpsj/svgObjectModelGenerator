{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "/global",
	"title": "global",
	"description": "The global object",
	"type": "object",
	"properties": {
		"colors": {
			"description": "A author specified color map with `id` as key",
			"patternProperties": {
				"^[//W//w]+": {
					"description": "A style object",
					"type": "object",
					"$ref": "/color"
				}
			}
		},
		"meta": {
			"type": "object"
		},
		"styles": {
			"description": "A author specified style block map with `id` as key",
			"patternProperties": {
				"^[//W//w]+": {
					"description": "A style object",
					"type": "object",
					"$ref": "/style"
				}
			}
		},
		"gradients": {
			"description": "A gradient object map with `id` as key",
			"patternProperties": {
				"^[//W//w]+": {
					"description": "A gradient object",
					"type": "object",
					"$ref": "#gradient"
				}
			}
		},
		"patterns": {
			"description": "A pattern object map with `id` as key",
			"patternProperties": {
				"^[//W//w]+": {
					"description": "A pattern object",
					"type": "object",
					"$ref": "#pattern"
				}
			}
		},
		"masks": {
			"description": "A mask object map with `id` as key",
			"patternProperties": {
				"^[//W//w]+": {
					"description": "A mask object",
					"type": "object",
					"$ref": "#mask"
				}
			}
		},
		"clipPaths": {
			"description": "A clipping path object map with `id` as key",
			"patternProperties": {
				"^[//W//w]+": {
					"description": "A clipPath object",
					"type": "object",
					"$ref": "#clipPath"
				}
			}
		},
		"filters": {
			"description": "A filter object map with `id` as key",
			"patternProperties": {
				"^[//W//w]+": {
					"description": "A filter object",
					"type": "object",
					"$ref": "/filter"
				}
			}
		},
		"symbols": {
			"description": "A symbol object map with `id` as key",
			"patternProperties": {
				"^[//W//w]+": {
					"description": "A symbol object",
					"type": "object",
					"$ref": "#symbol"
				}
			}

		}
	},
	"additionalProperties": false,
	"definitions": {
		"colorStop": {
			"description": "A gradient color stop",
			"meta": {
				"type": "object"
			},
			"type": "object",
			"properties": {
				"color": {
					"oneOf": [
						{ "$ref": "/colorRef" },
						{ "$ref": "/color" }
					]
				},
				"offset": {
					"type": "number",
					"minimum": 0,
					"maximum": 1
				},
				"midpoint": {
					"type": "number",
					"minimum": 0,
					"maximum": 1
				}
			},
			"additionalProperties": false,
			"required": [ "color", "offset" ]
		},
		"gradient": {
			"description": "A gradient object",
			"type": "object",
			"properties": {
				"interpolation": {
					"enum": [ "lienar", "euclidean", "spline" ]
				},
				"meta": {
					"type": "object"
				},
				"name": {
					"description": "The specified name of the gradient",
					"type": "string"
				},
				"stops": {
					"description": "A collections of color stop objects",
					"type": "array",
					"items": { "$ref": "#colorStop" },
					"minItems": 1
				},
				"type": {
					"enum": [ "linear", "radial" ]
				}
			},
			"additionalProperties": false,
			"required": [ "type", "stops" ]
		},
		"pattern": {
			"description": "A pattern object",
			"properties": {
				"bounds": {
					"description": "The bounds of the tile in the coordinate space defined by `patternUnits`",
					"$ref": "/bounds"
				},
				"children": {
					"description": "The content of a pattern tile",
					"type": "array",
					"items": {
						"oneOf": [
							{ "$ref": "/rect" },
							{ "$ref": "/circle" },
							{ "$ref": "/ellipse" },
							{ "$ref": "/line" },
							{ "$ref": "/polyline" },
							{ "$ref": "/polygon" },
							{ "$ref": "/text" },
							{ "$ref": "/image" },
							{ "$ref": "/reference" },
							{ "$ref": "/group" }
						]
					}
				},
				"meta": {
					"type": "object"
				},
				"name": {
					"description": "A name of the object within the application. A name doesn't need to be unique",
					"type": "string"
				},
				"units": {
					"description": "Global or object coordinate space for the boundaries of the mask",
					"enum": [ "objectBoundingBox", "userSpaceOnUse" ]
				},
				"contentUnits": {
					"description": "Global or object coordinate space for the children of the mask",
					"enum": [ "objectBoundingBox", "userSpaceOnUse" ]
				},
				"type": {
					"enum": "pattern"
				},
				"viewBox": {
					"description": "An additional transformation of the childrens' coordinate space. The position translates, the dimension scales",
					"type": "object",
					"$ref": "/bounds"
				}
			},
			"additionalProperties": false,
			"required": [ "bounds", "children", "type" ]
		},
		"clipPath": {
			"description": "A clipPath object",
			"properties": {
				"children": {
					"description": "The content of a pattern tile",
					"type": "array",
					"items": {
						"oneOf": [
							{ "$ref": "/rect" },
							{ "$ref": "/circle" },
							{ "$ref": "/ellipse" },
							{ "$ref": "/line" },
							{ "$ref": "/polyline" },
							{ "$ref": "/polygon" },
							{ "$ref": "/text" },
							{ "$ref": "/image" },
							{ "$ref": "/reference" },
							{ "$ref": "/group" }
						]
					}
				},
				"meta": {
					"type": "object"
				},
				"name": {
					"description": "A name of the object within the application. A name doesn't need to be unique",
					"type": "string"
				},
				"type": {
					"enum": [ "clipPath" ]
				}
			},
			"additionalProperties": false,
			"required": [ "children", "type" ]
		},
		"mask": {
			"description": "A mask object",
			"properties": {
				"bounds": {
					"description": "The hard clipping boundaries of the mask",
					"$ref": "/bounds"
				},
				"children": {
					"description": "The content of a pattern tile",
					"type": "array",
					"items": {
						"oneOf": [
							{ "$ref": "/rect" },
							{ "$ref": "/circle" },
							{ "$ref": "/ellipse" },
							{ "$ref": "/line" },
							{ "$ref": "/polyline" },
							{ "$ref": "/polygon" },
							{ "$ref": "/text" },
							{ "$ref": "/image" },
							{ "$ref": "/reference" },
							{ "$ref": "/group" }
						]
					}
				},
				"clip": {
					"description": "If `true` the object gets clipped to the art bounds of the mask's content",
					"type": "boolean"
				},
				"invert": {
					"description": "If `true` the luminance/opacity of the mask content gets inverted",
					"type": "boolean"
				},
				"kind": {
					"enum": [ "luminosity", "opacity" ]
				},
				"units": {
					"description": "Global or object coordinate space for the boundaries of the mask",
					"enum": [ "objectBoundingBox", "userSpaceOnUse" ]
				},
				"contentUnits": {
					"description": "Global or object coordinate space for the children of the mask",
					"enum": [ "objectBoundingBox", "userSpaceOnUse" ]
				},
				"meta": {
					"type": "object"
				},
				"name": {
					"description": "A name of the object within the application. A name doesn't need to be unique",
					"type": "string"
				},
				"type": {
					"enum": [ "mask" ]
				}
			},
			"additionalProperties": false,
			"required": [ "children" ]
		},
		"symbol": {
			"description": "A symbol object",
			"properties": {
				"children": {
					"description": "The content of a pattern tile",
					"type": "array",
					"items": {
						"oneOf": [
							{ "$ref": "/rect" },
							{ "$ref": "/circle" },
							{ "$ref": "/ellipse" },
							{ "$ref": "/line" },
							{ "$ref": "/polyline" },
							{ "$ref": "/polygon" },
							{ "$ref": "/text" },
							{ "$ref": "/image" },
							{ "$ref": "/reference" },
							{ "$ref": "/group" }
						]
					}
				},
				"meta": {
					"type": "object"
				},
				"name": {
					"description": "A name of the object within the application. A name doesn't need to be unique",
					"type": "string"
				},
				"type": {
					"enum": [ "symbol" ]
				},
				"viewBox": {
					"description": "The view box of the group",
					"type": "object",
					"$ref": "/bounds"
				}
			},
			"additionalProperties": false,
			"required": [ "children", "type" ]
		}
	}
}
